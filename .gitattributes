# Unreal Engine file types.
*.uasset filter=lfs diff=lfs merge=lfs -text
*.umap filter=lfs diff=lfs merge=lfs -text
# Raw Content file types.
*.fbx filter=lfs diff=lfs merge=lfs -text
*.3ds filter=lfs diff=lfs merge=lfs -text
*.psd filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text
*.xcf filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
# Anything in `/RawContent` dir.
/RawContent/**/* filter=lfs diff=lfs merge=lfs -text
Binaries/Win64/BlackBoxTurret.exe filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/ThirdParty/**/*.a filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/ThirdParty/**/*.lib filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/ThirdParty/**/*.dll filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/ThirdParty/**/*.so filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/ThirdParty/**/*.dylib filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/Binaries/**/*.dll filter=lfs diff=lfs merge=lfs -text
Plugins/Puerts/** filter=lfs diff=lfs merge=lfs -text
*.ts !text !filter !merge !diff
